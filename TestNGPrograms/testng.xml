		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="EdgeTest">
		<parameter name="browser" value="Edge"></parameter>
		<classes>
			<class name="demo.CBT" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
